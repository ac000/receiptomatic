		<tmpl_if name = "new_row" value = "yes">
		<tr>
		</tmpl_if>
</tmpl_loop>
	</tr>
	</tbody>
	</table>
	</center>

	<a href = 'http://ri.opentechlabs.net/tagged_receipts/?page_no=<tmpl_var name = "prev_page" />'> Prev Page </a>
